    "@mui/lab": "^5.0.0-alpha.157",
    "@mui/material": "^5.15.1",
    "@reduxjs/toolkit": "^2.0.1",
    "axios": "^1.6.2",
    "date-fns": "^3.0.6",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.48.2",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
